import math

def angle_MBC(ab, bc):
    # Calculate the hypotenuse (Pythagoras theorem)
    ac = math.sqrt(ab**2 + bc**2)
    
    # Calculate the angle using cosine rule
    # cos(Î¸) = AB / AC, where AB = ab, AC = ac
    cos_theta = ab / ac
    
    # Calculate the angle in radians first. it is the process in pyhton. fraom radiants to degree we need to calculate
    angle_rad = math.acos(cos_theta)
    
    # Convert radians to degrees
    angle_deg = math.degrees(angle_rad)
    
    # Return the rounded angle
    return round(angle_deg)

# Get input values
ab = int(input())  # Side AB
bc = int(input())  # Side BC

# Calculate the angle
MBC = angle_MBC(ab, bc)

# Print the result with the degree symbol
print((str(MBC)), chr(176), sep='') // chr(176) print degree and its str type data
